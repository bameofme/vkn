    <div class="container">
        <h1 class="text-center my-4">PC Status and Configuration</h1>
        <ul class="nav nav-tabs">
            <li class="nav-item">
                <a class="nav-link active" id="status-tab" href="#" onclick="showTab('status')">Status</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="config-tab" href="#" onclick="showTab('config')">Config</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="texteditor-tab" href="#" onclick="redirectToTextEditor()">Text Editor</a>
            </li>
