        <div id="status" class="tab-content active">
            <div class="status-container mt-4">
                <div class="status-item card my-2 p-3">
                    <div class="row">
                        <div class="col">CPU Usage:</div>
                        <div class="col text-right"><span id="cpu-usage"></span>%</div>
                    </div>
                </div>
                <div class="status-item card my-2 p-3">
                    <div class="row">
                        <div class="col">Memory Usage:</div>
                        <div class="col text-right"><span id="memory-usage"></span>%</div>
                    </div>
                </div>
                <div class="status-item card my-2 p-3">
                    <div class="row">
                        <div class="col">Disk Usage:</div>
                        <div class="col text-right"><span id="disk-usage"></span>%</div>
                    </div>
                </div>
                <div class="status-item card my-2 p-3">
                    <div class="row">
                        <div class="col">Uptime:</div>
                        <div class="col text-right"><span id="uptime"></span></div>
                    </div>
                </div>
                <canvas id="cpuChart" width="400" height="200"></canvas>
            </div>
        </div>
